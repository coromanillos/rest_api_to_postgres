generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model IntradayData {
	id 			Int 		@id @default(autoincrement())
	timestamp 	DateTime 	@map("timestamp")
	open		Float		@map("open")
	high		Float		@map("high")
	low			Float		@map("low")
	close		Float		@map("close")
	volume		Int     	@map("volume")
	createdAt 	DateTime	@default(now()) @map("created_at")
}